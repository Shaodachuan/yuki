<!-- 视频页面 -->

<div class="wrap max game-wrapper bgGrey mt-25 pd-4">
    <!-- home-title -->
    <section class="home-title">
        <h1><%= page.title%></h1>
        <span><%= page.subTitle%></span>
    </section>
    
    <!-- lolTitle -->
    <div class="mt-10">
        <h4 class="home-title-item mb-3"><%= page.lolTitle%></h4>
        <div class="home-box">
            <%  page.lolList.forEach((item) => { %>
            <div class="home-item-wrapper">
                <div class="home-item">
                    <img src="<%- item.imgSrc%>">
                    <div class="home-right">
                        <a href="<%- item.link%>"><%= item.title%></a>
                        <div><%= item.subTitle%></div>
                    </div>
                </div>
            </div>
            <%  }) %>
       </div>
    </div>
    
    <!-- DNF -->
    <div class="mt-10">
        <h4 class="home-title-item mb-3"><%= page.dnfTitle%></h4>
        <div class="home-box">
            <%  page.dnfList.forEach((item) => { %>
            <div class="home-item-wrapper">
                <div class="home-item">
                    <img src="<%- item.imgSrc%>">
                    <div class="home-right">
                        <a href="<%- item.link%>"><%= item.title%></a>
                        <div><%= item.subTitle%></div>
                    </div>
                </div>
            </div>
            <%  }) %>
       </div>
    </div>
</div>